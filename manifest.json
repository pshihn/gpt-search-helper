{
  "manifest_version": 3,
  "name": "GPT Search Helper",
  "version": "1.0",
  "description": "TODO: ....",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "webNavigation"
  ],
  "background": {
    "service_worker": "lib/background.js"
  },
  "content_scripts": [
    {
      "js": [
        "lib/chat-content.js"
      ],
      "matches": [
        "https://chat.openai.com/chat/",
        "https://chat.openai.com/chat/*"
      ]
    }
  ]
}